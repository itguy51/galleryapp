<h1>Listing imageitems</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @imageitems.each do |imageitem| %>
  <tr>
    <td><%= imageitem.title %></td>
    <td><%= imageitem.desc %></td>
    <td><%= link_to 'Show', imageitem %></td>
    <td><%= link_to 'Edit', edit_imageitem_path(imageitem) %></td>
    <td><%= link_to 'Destroy', imageitem, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Imageitem', new_imageitem_path %>
